{
    "name": "qeb-hwt-website",
    "version": "0.1.0",
    "description": "An open source build scaffolding utility for web apps.",
    "main": "index.js",
    "repository": "https://github.com/patternfly/patternfly-react-seed.git",
    "license": "MIT",
    "private": true,
    "scripts": {
        "prebuild": "yarn clean",
        "build": "webpack --config webpack.prod.js",
        "start": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
        "test": "jest",
        "tslint": "tslint -c ./tslint.json --project .",
        "eslint": "eslint --ext .tsx,.js ./src/",
        "lint": "yarn tslint && yarn eslint",
        "format": "prettier --check --write ./src/**/*.{tsx,ts,js}",
        "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
        "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
        "clean": "rimraf dist",
        "storybook": "start-storybook -p 6006",
        "build:storybook": "build-storybook"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^5.3.21",
        "@storybook/addon-info": "^5.3.21",
        "@storybook/addon-links": "^5.3.21",
        "@storybook/addons": "^5.3.21",
        "@storybook/react": "^5.3.21",
        "@types/enzyme": "^3.10.7",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^24.9.1",
        "@types/node": "^12.12.62",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "@types/storybook__react": "^4.0.2",
        "@types/victory": "^33.1.4",
        "@types/webpack": "^4.41.22",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "css-loader": "^3.6.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "enzyme-to-json": "^3.6.1",
        "eslint": "^6.8.0",
        "eslint-plugin-patternfly-react": "^0.2.3",
        "eslint-plugin-react-hooks": "^2.5.1",
        "file-loader": "^4.3.0",
        "html-webpack-plugin": "^3.2.0",
        "imagemin": "^7.0.1",
        "jest": "^24.1.0",
        "mini-css-extract-plugin": "^0.8.2",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "prettier": "^1.19.1",
        "prop-types": "^15.6.1",
        "raw-loader": "^3.1.0",
        "react": "^16.13.1",
        "react-axe": "^3.5.3",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^16.13.1",
        "regenerator-runtime": "^0.13.7",
        "rimraf": "^3.0.2",
        "style-loader": "^1.2.1",
        "svg-url-loader": "^3.0.3",
        "terser-webpack-plugin": "^2.3.8",
        "ts-jest": "^24.3.0",
        "ts-loader": "^6.2.2",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-react": "^4.0.0",
        "tslint-react-hooks": "^2.2.2",
        "typescript": "^3.9.7",
        "url-loader": "^2.3.0",
        "webpack": "^4.44.2",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.1.4"
    },
    "dependencies": {
        "@iarna/toml": "^2.2.5",
        "@patternfly/react-core": "^3.158.4",
        "@patternfly/react-icons": "^3.15.17",
        "@patternfly/react-styles": "^3.7.14",
        "axios": "^0.21.1",
        "git-revision-webpack-plugin": "^3.0.6",
        "humanize-duration": "^3.23.1",
        "query-string": "^6.13.3",
        "react-router-dom": "^5.2.0",
        "react-router-last-location": "^2.0.1",
        "react-syntax-highlighter": "^11.0.2"
    }
}
